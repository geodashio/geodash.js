{
  "name": "geodash.js",
  "version": "0.0.1",
  "description": "Low-level Javascript API for GeoDash",
  "keywords": [
    "map",
    "mapping",
    "geodash"
  ],
  "homepage": "http://geodash.io/",
  "scripts": {
    "build:docs": "mkdir docs; gulp docs",
    "build:js": "browserify src/index.js --outfile dist/geodash.js",
    "uglify": "cat dist/geodash.js | uglifyjs > dist/geodash.min.js",
    "build:code": "mkdir dist; npm run build:js && npm run uglify",
    "build": "npm run build:docs; npm run build:code; gulp tests",
    "jshint": "jshint src",
    "tests": "npm run jshint; gulp tests"
  },
  "main": "dist/geodash.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/geodashio/geodash.js"
  },
  "license": "BSD-3-Clause",
  "author": "GeoDash Developers",
  "bugs": {
    "url": "https://github.com/geodashio/geodash.js/issues"
  },
  "dependencies": {
    "geodash-diff": "*",
    "geodash-extract": "*",
    "geodash-expand": "*",
    "geodash-flatten": "*"
  },
  "devDependencies": {
    "browserify": "*",
    "browserify-shim": "*",
    "gulp": "*",
    "gulp-jsdoc3": "^0.3.0",
    "jasmine-core": "^2.5.2",
    "jshint": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-jasmine": "*",
    "karma-phantomjs-launcher": "*",
    "karma-spec-reporter": "*",
    "karma-commonjs": "*",
    "karma-requirejs": "*",
    "karma-browserify": "*",
    "karma-mocha": "*",
    "karma-commonjs-preprocessor": "*",
    "uglify-js": "*",
    "requirejs": "*",
    "watchify": "*",
    "mocha": "*",
    "browser-stdout": "*",
    "angular": "*",
    "angular-mocks": "*",
    "jquery": "1.9.1"
  }
}
